<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hub.HUBActivity">

  <TextView
      android:id="@+id/textHub"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="HUB"
      android:textSize="30sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.062" />

  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/lottieAnimationViewBuh"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="gone"
      app:lottie_autoPlay="false"
      app:lottie_fileName="buh.lottie"
      app:lottie_loop="false"
      app:layout_constraintBottom_toTopOf="@+id/textHub"
      app:layout_constraintEnd_toEndOf="@+id/textHub"
      app:layout_constraintStart_toStartOf="@+id/textHub"
      app:layout_constraintTop_toTopOf="@+id/textHub"
      />


  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/lottieAnimationView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="gone"
      app:lottie_autoPlay="false"
      app:lottie_fileName="transicion_activities.lottie"
      app:lottie_loop="false"
      tools:ignore="MissingConstraints" />

  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/lottieAnimationViewSpark"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="invisible"
      app:lottie_autoPlay="false"
      app:lottie_fileName="spark.lottie"
      app:lottie_loop="false"
      app:layout_constraintBottom_toBottomOf="@+id/textViewUsuario"
      app:layout_constraintEnd_toEndOf="@+id/textViewUsuario"
      app:layout_constraintStart_toStartOf="@+id/textViewUsuario"
      app:layout_constraintTop_toTopOf="@+id/textViewUsuario" />

  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/lottieAnimationViewRabbit"
      android:layout_width="99dp"
      android:layout_height="99dp"
      android:visibility="visible"
      app:layout_constraintBottom_toTopOf="@+id/playPauseButton"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0"
      app:lottie_autoPlay="false"
      app:lottie_fileName="rabbit.lottie"
      app:lottie_loop="true"
      tools:ignore="MissingConstraints" />

  <ImageView
      android:id="@+id/imagePig"
      android:layout_width="68dp"
      android:layout_height="68dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.084"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.295"
      app:srcCompat="@drawable/dadovictoria" />

  <TextView
      android:id="@+id/textPig"
      android:layout_width="39dp"
      android:layout_height="27dp"
      android:text="Pig"
      android:textAlignment="center"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.115"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imagePig"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textViewUsuario"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.16" />

  <TextView
      android:id="@+id/camaraImage"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Camara"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/CamaraImage"
      app:layout_constraintVertical_bias="0.0" />

  <ImageView
      android:id="@+id/CamaraImage"
      android:layout_width="75dp"
      android:layout_height="69dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.296"
      app:srcCompat="@android:drawable/ic_menu_camera" />

  <ImageView
      android:id="@+id/VideoPlayerImage"
      android:layout_width="69dp"
      android:layout_height="67dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.877"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.298"
      app:srcCompat="@android:drawable/ic_media_play" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="VideoPlayer"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.892"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/VideoPlayerImage"
      app:layout_constraintVertical_bias="0.004" />

  <Button
      android:id="@+id/playPauseButton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Play"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.959" />

  <ImageView
      android:id="@+id/musicPlayerImage"
      android:layout_width="69dp"
      android:layout_height="62dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.081"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.454"
      app:srcCompat="@drawable/iconoappmusica" />

  <TextView
      android:id="@+id/musicPlayer"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="11dp"
      android:text="MusicPlayer"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.082"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/musicPlayerImage"
      app:layout_constraintVertical_bias="0.0" />

  <ImageView
      android:id="@+id/randomMusic"
      android:layout_width="195dp"
      android:layout_height="133dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.707"
      app:srcCompat="@android:drawable/ic_menu_compass" />

  <ImageView
      android:id="@+id/ballGameImage"
      android:layout_width="71dp"
      android:layout_height="66dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.457"
      app:srcCompat="@android:drawable/ic_notification_overlay" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="BallGame"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/ballGameImage"
      app:layout_constraintVertical_bias="0.02" />

</androidx.constraintlayout.widget.ConstraintLayout>